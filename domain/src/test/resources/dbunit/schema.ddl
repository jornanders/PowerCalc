
    drop table maaleavlesning if exists;

    drop table regning if exists;

    drop table regning_linje if exists;

    create table maaleavlesning (
        avlesningId integer generated by default as identity (start with 1),
        dato date not null,
        punkt varchar(255) not null,
        stand integer not null,
        primary key (avlesningId)
    );

    create table regning (
        regningId integer generated by default as identity (start with 1),
        fradato date not null,
        tildato date not null,
        primary key (regningId)
    );

    create table regning_linje (
        regningLinjeId integer generated by default as identity (start with 1),
        avsender varchar(255) not null,
        sumverdi double not null,
        regningId integer,
        primary key (regningLinjeId)
    );

    alter table regning_linje 
        add constraint FK82B603D5D5EC2E04 
        foreign key (regningId) 
        references regning;
